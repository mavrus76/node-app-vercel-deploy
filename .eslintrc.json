{
  "parser": "@babel/eslint-parser",

  "extends": ["prettier", "eslint:recommended", "plugin:node/recommended"],
  "env": {
    "node": true,
    "jest/globals": true,
    "es2022": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "requireConfigFile": false
  },
  "plugins": ["prettier", "jest"],
  "rules": {
    "no-process-exit": 0
  },
  "settings": {}
}
